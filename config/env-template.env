# 阿里云Flink Catalog服务环境变量模板
# 复制此文件为 .env 并填入实际值

# 阿里云Flink配置
ALIYUN_FLINK_ENDPOINT=https://your-flink-endpoint
ALIYUN_REGION=cn-hangzhou
ALIYUN_ACCESS_KEY_ID=your-access-key-id
ALIYUN_ACCESS_KEY_SECRET=your-access-key-secret

# MySQL数据库配置
MYSQL_URL=jdbc:mysql://localhost:3306/flink_metadata
MYSQL_USERNAME=root
MYSQL_PASSWORD=your-mysql-password

# 用户数据库配置
USER_DB_URL=jdbc:mysql://localhost:3306/user_db
USER_DB_USERNAME=root
USER_DB_PASSWORD=your-user-db-password

# 指标数据库配置
METRICS_DB_URL=jdbc:mysql://localhost:3306/metrics_db
METRICS_DB_USERNAME=root
METRICS_DB_PASSWORD=your-metrics-db-password

# Kafka配置
KAFKA_BOOTSTRAP_SERVERS=localhost:9092
KAFKA_SECURITY_PROTOCOL=PLAINTEXT
KAFKA_SASL_MECHANISM=PLAIN
KAFKA_USERNAME=your-kafka-username
KAFKA_PASSWORD=your-kafka-password

# 阿里云OSS配置
OSS_ENDPOINT=your-oss-endpoint
OSS_BUCKET=your-bucket-name
OSS_ACCESS_KEY_ID=your-oss-access-key-id
OSS_ACCESS_KEY_SECRET=your-oss-access-key-secret
OSS_REGION=cn-hangzhou

# Redis缓存配置（可选）
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your-redis-password

# 安全配置
MCP_ENCRYPTION_KEY=your-encryption-key
ENCRYPTION_KEY_FILE=/etc/mcp/encryption.key
AUDIT_LOG_FILE=/var/log/mcp/audit.log

# 监控配置
ERROR_NOTIFICATION_EMAIL=admin@example.com

# 开发配置
DEBUG_ENABLED=false
LOG_LEVEL=INFO
